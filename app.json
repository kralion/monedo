{
  "expo": {
    "name": "monedo",
    "slug": "monedo",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "monedo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#41D29B"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#41D29B"
      },
      "package": "com.joanpaucar.monedo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-dynamic-app-icon",
        {
          "blue": {
            "image": "./assets/icons/blue.png",
            "prerendered": true
          },
          "nightblue": {
            "image": "./assets/icons/nightblue.png",
            "prerendered": true
          },
          "default": {
            "image": "./assets/icons/default.png",
            "prerendered": true
          }
        }
      ],
      [
        "expo-quick-actions",
        {
          "androidIcons": {
            "add_circle": {
              "foregroundImage": "./assets/adaptive-add.png",
              "backgroundColor": "#41D29B"
            },
            "bar_chart": {
              "foregroundImage": "./assets/adaptive-chart.png",
              "backgroundColor": "#41D29B"
            }
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "resizeMode": "cover",
          "image": "./assets/images/splash.png",
          "dark": {
            "image": "./assets/splash-dark.png",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0c4479ab-e0fa-4283-bc37-c0de1370eae1"
      }
    },
    "owner": "joanpaucar"
  }
}
